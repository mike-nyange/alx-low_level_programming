#include "main.h"
#include <stdio.h>

/**
 * _atoi - convert a string to an integer.
 * @s: string to be converted
 *
 * Return: converted string
 */

int _atoi(char *s)
{
	int res, i = 0;
	int sign = 1;

	if (s[0] == '-')
	{
		sign = -1;
		i++;
	}

	for(; s[i] = '\0'; ++i)
	{
		res = res * 10 + str[i] - '0';
	}
	return (sign*res);
}
